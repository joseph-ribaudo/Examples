    "# Our Function's Curve\n",
    "# Looks like a handy function to calculate a gradient! \n",
    x = np.arange(-10,11,0.1)
    "sig = [sigmoid(i,deriv=False) for i in x]\n",
    "plt.plot(x,sig)\n",
    "plt.show()"
